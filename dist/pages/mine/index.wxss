page {
  background-color: var(--defaultBgColor);
}
.header {
  margin-bottom: 10px;
}
.ui-blur {
  border-radius: 8px;
}
.headimg {
  width: 66px;
  height: 66px;
  border-radius: 100rpx;
  margin-right: 10px;
}
.nickname {
  color: #fff;
  font-size: 16px;
  font-weight: 650;
}
.no {
  display: flex;
  align-items: center;
  color: #aaa;
  margin-top: 5px;
}
.header-info-line {
  padding: 5px 5px 5px 15px;
  color: #fff;
  display: flex;
  align-items: center;
  font-size: 13px;
  justify-content: flex-end;
  background-color: rgba(0, 0, 0, 0.38);
  border-top-left-radius: 100px;
  border-bottom-left-radius: 100px;
}
.level {
  margin-bottom: 8px;
}
.level image {
  width: 19px;
  height: 19px;
  margin-right: 3px;
}
.level text {
  margin-left: 5px;
  color: coral;
  font-weight: 600;
}
.area-agent text {
  color: coral;
}
.are-agent-dialog {
  background-color: #fff;
  height: 400px;
}
.area-agent-form {
  padding: 15px;
}
.area-agent-form-row {
  display: flex;
  height: 56px;
  align-items: center;
}
.area-agent-form-label {
  display: flex;
  width: 80px;
  justify-content: center;
  height: 100%;
  align-items: center;
}
.area-agent-form-value {
  display: flex;
  flex: 1;
  height: 100%;
  align-items: center;
}
.area-agent-form-value input {
  line-height: 40px;
  height: 40px;
}
.col-menu-icon {
  width: 20px;
  height: 20px;
}
.main-cate-icon {
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
}
.contact-btn::after {
  border: none;
}
.contact-btn {
  background-color: #fff;
  border-radius: 0;
  padding: 0;
  margin: 0;
  color: #000;
}
.col-menu-container {
  padding: 10px;
}
.col-menu-wrap {
  background-color: #fff;
  border-radius: 5px;
}
.order-conatainer {
  padding: 10px;
}
.order-wrap {
  padding: 10px;
  background-color: #fff;
  border-radius: 5px;
}
.order-header {
  display: flex;
  padding: 5px 0 15px;
}
.order-header-name {
  display: flex;
  flex: 1;
  font-weight: 650;
  font-size: 15px;
  align-items: center;
}
.order-header-more {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: flex-end;
  font-size: 13px;
  color: #666;
}
.order-header-more image {
  width: 12px;
  height: 12px;
}
.order-list {
  display: flex;
}
.order-item {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.order-item-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 35px;
  position: relative;
  width: 35px;
  margin: 8px 0;
}
.order-item-icon image {
  width: 100%;
  height: 100%;
}
.badge {
  position: absolute;
  right: -10px;
  top: -10px;
}
.order-item-name {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 13px;
  color: #666;
  height: 25px;
}
